<view>
    <z-navigation id="navigationBar" placeholderBg="" color="{{color}}" title="{{title}}" back="{{true}}"
                  background="{{barBg}}" fixed="{{fixed}}" backConfirm="{{backConfirm}}" backStyle="{{backStyle}}"
                  backEvent="{{backEvent}}" bind:back="backEvent" backHomeEvent="{{backHomeEvent}}"
                  bind:backHome="backHomeEvent"></z-navigation>
    <view id="container" bindtouchstart='touchstart' bindtouchmove='touchmove' style='height:200vh;'>
        <view bindtap="bindViewTap" style="width: 750rpx:height:750rpx;">页面容器</view>
    </view>
</view>

